{
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "no-duplicate-selectors": true,
    "block-no-empty": true,
    "length-zero-no-unit": true,
    "import-notation": "string",
    "color-hex-length": "long",
    "selector-class-pattern": null,
    "scss/dollar-variable-pattern": null
  },
  "ignoreFiles": [
    "dist/*"
  ]
}
